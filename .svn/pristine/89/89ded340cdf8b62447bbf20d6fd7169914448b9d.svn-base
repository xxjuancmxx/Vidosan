package entidad;

/**
 *
 * @author samuel.gonzalez
 */
public class TipoCurso {
    
    private int idtipocurso;
    private String codigo;
    private String nombre;

    public TipoCurso() {
    }

    public TipoCurso(int idtipocurso, String codigo, String nombre) {
        this.idtipocurso = idtipocurso;
        this.codigo = codigo;
        this.nombre = nombre;
    }

    public int getIdtipocurso() {
        return idtipocurso;
    }

    public void setIdtipocurso(int idtipocurso) {
        this.idtipocurso = idtipocurso;
    }

    public String getCodigo() {
        return codigo;
    }

    public void setCodigo(String codigo) {
        this.codigo = codigo;
    }

    public String getNombre() {
        return nombre;
    }

    public void setNombre(String nombre) {
        this.nombre = nombre;
    }

    @Override
    public String toString() {
        return "TipoCurso{" + "idtipocurso=" + idtipocurso + ", codigo=" + codigo + ", nombre=" + nombre + '}';
    }
    
    
    
}
